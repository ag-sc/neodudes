@prefix :        <http://localhost:8000/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://localhost:8000/lemon.owl#> .

@base            <http://localhost:8000#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_pay_1_21 ;
  lemon:entry    :pay_1_21ed ;
  lemon:entry    :form_pay_1_21_preposition .

:to_pay_1_21 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_pay_1_21 ;
  lemon:otherForm      :form_pay_1_21s ;
  lemon:otherForm      :form_pay_1_21ed ;
  lemon:otherForm      :form_pay_1_21perfect ;
  lemon:sense          :pay_1_21_sense_1 ;
  lemon:synBehavior    :pay_1_21_frame_transitive .

:form_pay_1_21 a         lemon:Form ;
  lemon:writtenRep     "pay"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_pay_1_21s a    lemon:Form ;
  lemon:writtenRep "pays"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_pay_1_21ed a   lemon:Form ;
  lemon:writtenRep "payed"@en ;
  lexinfo:tense    lexinfo:past .

:form_pay_1_21perfect a   lemon:Form ;
  lemon:writtenRep "payed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:pay_1_21_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :pay_1_21_obj ;
  lexinfo:directObject     :pay_1_21_subj .

:pay_1_21_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :pay_1_21_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/currency> ;
  lemon:subjOfProp  :pay_1_21_obj ;
  lemon:objOfProp   :pay_1_21_subj ;
  lemon:condition   :pay_1_21_sense_1_condition .


:pay_1_21_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Currency> .


:pay_1_21_obj lemon:marker :form_pay_1_21_preposition .

## Prepositions ##
:form_pay_1_21_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
