@prefix :        <http://localhost:8000/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://localhost:8000/lemon.owl#> .

@base            <http://localhost:8000#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :establish_in_2_4 ;
  lemon:entry    :form_establish_in_2_4_preposition .

:establish_in_2_4 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_establish_in_2_4 ;
  lemon:otherForm      :form_establish_in_2_4_present3rd ;
  lemon:otherForm      :form_establish_in_2_4_past ;
  lemon:otherForm      :form_establish_in_2_4_perfect ;
  lemon:sense          :establish_in_2_4_sense_1 ;
  lemon:synBehavior    :establish_in_2_4_frame .

:form_establish_in_2_4 a           lemon:Form ;
  lemon:writtenRep     "establish"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_establish_in_2_4_present3rd a      lemon:Form ;
  lemon:writtenRep "establishes"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_establish_in_2_4_past a lemon:Form ;
  lemon:writtenRep  "established"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_establish_in_2_4_perfect a   lemon:Form ;
  lemon:writtenRep "established"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:establish_in_2_4_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :establish_in_2_4_subj ;
  lexinfo:prepositionalAdjunct :establish_in_2_4_obj .

:establish_in_2_4_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :establish_in_2_4_ontomap ;
  lemon:reference   <http://dbpedia.org/property/established> ;
  lemon:subjOfProp  :establish_in_2_4_subj ;
  lemon:objOfProp   :establish_in_2_4_obj ;
  lemon:condition   :establish_in_2_4_sense_1_condition .

:establish_in_2_4_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Agent> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#gYear> .


:establish_in_2_4_obj lemon:marker :form_establish_in_2_4_preposition .

## Prepositions ##
:form_establish_in_2_4_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "as"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

