@prefix :        <http://localhost:8000/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://localhost:8000/lemon.owl#> .

@base            <http://localhost:8000#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :founded_in_1_6 ;
  lemon:entry    :form_founded_in_1_6_preposition .

:founded_in_1_6 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_founded_in_1_6 ;
  lemon:otherForm      :form_founded_in_1_6_present3rd ;
  lemon:otherForm      :form_founded_in_1_6_past ;
  lemon:otherForm      :form_founded_in_1_6_perfect ;
  lemon:sense          :founded_in_1_6_sense_1 ;
  lemon:synBehavior    :founded_in_1_6_frame .

:form_founded_in_1_6 a           lemon:Form ;
  lemon:writtenRep     "found"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_founded_in_1_6_present3rd a      lemon:Form ;
  lemon:writtenRep "founds"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_founded_in_1_6_past a lemon:Form ;
  lemon:writtenRep  "founded"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_founded_in_1_6_perfect a   lemon:Form ;
  lemon:writtenRep "founded"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:founded_in_1_6_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :founded_in_1_6_subj ;
  lexinfo:prepositionalAdjunct :founded_in_1_6_obj .

:founded_in_1_6_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :founded_in_1_6_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/formationYear> ;
  lemon:subjOfProp  :founded_in_1_6_subj ;
  lemon:objOfProp   :founded_in_1_6_obj ;
  lemon:condition   :founded_in_1_6_sense_1_condition .

:founded_in_1_6_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#gYear> .


:founded_in_1_6_obj lemon:marker :form_founded_in_1_6_preposition .

## Prepositions ##
:form_founded_in_1_6_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

