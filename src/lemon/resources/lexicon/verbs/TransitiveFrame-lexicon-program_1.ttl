@prefix :        <http://localhost:8000/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://localhost:8000/lemon.owl#> .

@base            <http://localhost:8000#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_program_1 ;
  lemon:entry    :program_1ed ;
  lemon:entry    :form_program_1_preposition .

:to_program_1 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_program_1 ;
  lemon:otherForm      :form_program_1s ;
  lemon:otherForm      :form_program_1ed ;
  lemon:otherForm      :form_program_1perfect ;
  lemon:sense          :program_1_sense_1 ;
  lemon:synBehavior    :program_1_frame_transitive .

:form_program_1 a         lemon:Form ;
  lemon:writtenRep     "program"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_program_1s a    lemon:Form ;
  lemon:writtenRep "programs"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_program_1ed a   lemon:Form ;
  lemon:writtenRep "programmed"@en ;
  lexinfo:tense    lexinfo:past .

:form_program_1perfect a   lemon:Form ;
  lemon:writtenRep "programmed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:program_1_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :program_1_subj ;
  lexinfo:directObject     :program_1_obj .

:program_1_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :program_1_sense_1 ;
  lemon:reference   <http://dbpedia.org/property/programmer> ;
  lemon:subjOfProp  :program_1_obj ;
  lemon:objOfProp   :program_1_subj ;
  lemon:condition   :program_1_sense_1_condition .


:program_1_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Software> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:program_1_obj lemon:marker :form_program_1_preposition .

## Prepositions ##
:form_program_1_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

