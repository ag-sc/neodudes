@prefix :        <http://localhost:8000/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://localhost:8000/lemon.owl#> .

@base            <http://localhost:8000#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :locate_in_2 ;
  lemon:entry    :form_locate_in_2_preposition .

:locate_in_2 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_locate_in_2 ;
  lemon:otherForm      :form_locate_in_2_present3rd ;
  lemon:otherForm      :form_locate_in_2_past ;
  lemon:otherForm      :form_locate_in_2_perfect ;
  lemon:sense          :locate_in_2_sense_1 ;
  lemon:sense          :locate_in_2_sense_2 ;
  lemon:synBehavior    :locate_in_2_frame .

:form_locate_in_2 a           lemon:Form ;
  lemon:writtenRep     "locate"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_locate_in_2_present3rd a      lemon:Form ;
  lemon:writtenRep "locates"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_locate_in_2_past a lemon:Form ;
  lemon:writtenRep  "located"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_locate_in_2_perfect a   lemon:Form ;
  lemon:writtenRep "located"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:locate_in_2_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :locate_in_2_subj ;
  lexinfo:prepositionalAdjunct :locate_in_2_obj .

:locate_in_2_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :locate_in_2_ontomap ;
  lemon:reference   <http://dbpedia.org/property/location> ;
  lemon:subjOfProp  :locate_in_2_subj ;
  lemon:objOfProp   :locate_in_2_obj ;
  lemon:condition   :locate_in_2_sense_1_condition .

:locate_in_2_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/MilitaryUnit> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Location> .

:locate_in_2_sense_2 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :locate_in_2_ontomap ;
  lemon:reference   <http://dbpedia.org/property/location> ;
  lemon:subjOfProp  :locate_in_2_subj ;
  lemon:objOfProp   :locate_in_2_obj ;
  lemon:condition   :locate_in_2_sense_2_condition .

:locate_in_2_sense_2_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/MilitaryUnit> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Location> .


:locate_in_2_obj lemon:marker :form_locate_in_2_preposition .

## Prepositions ##
:form_locate_in_2_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

