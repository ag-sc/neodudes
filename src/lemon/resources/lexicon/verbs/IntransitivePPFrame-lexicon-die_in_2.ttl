@prefix :        <http://localhost:8000/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://localhost:8000/lemon.owl#> .

@base            <http://localhost:8000#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :die_in_2 ;
  lemon:entry    :form_die_in_2_preposition .

:die_in_2 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_die_in_2 ;
  lemon:otherForm      :form_die_in_2_present3rd ;
  lemon:otherForm      :form_die_in_2_past ;
  lemon:otherForm      :form_die_in_2_perfect ;
  lemon:sense          :die_in_2_sense_1 ;
  lemon:synBehavior    :die_in_2_frame .

:form_die_in_2 a           lemon:Form ;
  lemon:writtenRep     "die"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_die_in_2_present3rd a      lemon:Form ;
  lemon:writtenRep "dies"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_die_in_2_past a lemon:Form ;
  lemon:writtenRep  "died"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_die_in_2_perfect a   lemon:Form ;
  lemon:writtenRep "died"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:die_in_2_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :die_in_2_subj ;
  lexinfo:prepositionalAdjunct :die_in_2_obj .

:die_in_2_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :die_in_2_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/deathPlace> ;
  lemon:subjOfProp  :die_in_2_subj ;
  lemon:objOfProp   :die_in_2_obj ;
  lemon:condition   :die_in_2_sense_1_condition .

:die_in_2_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Writer> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Place> .


:die_in_2_obj lemon:marker :form_die_in_2_preposition .

## Prepositions ##
:form_die_in_2_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

