@prefix :        <http://localhost:8000/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://localhost:8000/lemon.owl#> .

@base            <http://localhost:8000#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :initially_use_for_1 ;
  lemon:entry    :form_initially_use_for_1_preposition .

:initially_use_for_1 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_initially_use_for_1 ;
  lemon:otherForm      :form_initially_use_for_1_present3rd ;
  lemon:otherForm      :form_initially_use_for_1_past ;
  lemon:otherForm      :form_initially_use_for_1_perfect ;
  lemon:sense          :initially_use_for_1_sense_1 ;
  lemon:synBehavior    :initially_use_for_1_frame .

:form_initially_use_for_1 a           lemon:Form ;
  lemon:writtenRep     "initially use"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_initially_use_for_1_present3rd a      lemon:Form ;
  lemon:writtenRep "initially uses"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_initially_use_for_1_past a lemon:Form ;
  lemon:writtenRep  "initially used"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_initially_use_for_1_perfect a   lemon:Form ;
  lemon:writtenRep "initially used"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:initially_use_for_1_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :initially_use_for_1_subj ;
  lexinfo:prepositionalAdjunct :initially_use_for_1_obj .

:initially_use_for_1_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :initially_use_for_1_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/initiallyUsedFor> ;
  lemon:subjOfProp  :initially_use_for_1_subj ;
  lemon:objOfProp   :initially_use_for_1_obj ;
  lemon:condition   :initially_use_for_1_sense_1_condition .

:initially_use_for_1_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#string> .


:initially_use_for_1_obj lemon:marker :form_initially_use_for_1_preposition .

## Prepositions ##
:form_initially_use_for_1_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "for"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

