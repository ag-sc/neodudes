@prefix :        <http://localhost:8000/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://localhost:8000/lemon.owl#> .

@base            <http://localhost:8000#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :flow_through_1 ;
  lemon:entry    :form_flow_through_1_preposition .

:flow_through_1 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_flow_through_1 ;
  lemon:otherForm      :form_flow_through_1_present3rd ;
  lemon:otherForm      :form_flow_through_1_past ;
  lemon:otherForm      :form_flow_through_1_perfect ;
  lemon:sense          :flow_through_1_sense_1 ;
  lemon:synBehavior    :flow_through_1_frame .

:form_flow_through_1 a           lemon:Form ;
  lemon:writtenRep     "flow"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_flow_through_1_present3rd a      lemon:Form ;
  lemon:writtenRep "flows"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_flow_through_1_past a lemon:Form ;
  lemon:writtenRep  "flowed"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_flow_through_1_perfect a   lemon:Form ;
  lemon:writtenRep "flown"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:flow_through_1_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :flow_through_1_subj ;
  lexinfo:prepositionalAdjunct :flow_through_1_obj .

:flow_through_1_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :flow_through_1_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/city> ;
  lemon:subjOfProp  :flow_through_1_subj ;
  lemon:objOfProp   :flow_through_1_obj ;
  lemon:condition   :flow_through_1_sense_1_condition .

:flow_through_1_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/City> .


:flow_through_1_obj lemon:marker :form_flow_through_1_preposition .

## Prepositions ##
:form_flow_through_1_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "through"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

