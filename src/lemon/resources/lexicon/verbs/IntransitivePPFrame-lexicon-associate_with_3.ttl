@prefix :        <http://localhost:8000/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://localhost:8000/lemon.owl#> .

@base            <http://localhost:8000#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :associate_with_3 ;
  lemon:entry    :form_associate_with_3_preposition .

:associate_with_3 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_associate_with_3 ;
  lemon:otherForm      :form_associate_with_3_present3rd ;
  lemon:otherForm      :form_associate_with_3_past ;
  lemon:otherForm      :form_associate_with_3_perfect ;
  lemon:sense          :associate_with_3_sense_1 ;
  lemon:synBehavior    :associate_with_3_frame .

:form_associate_with_3 a           lemon:Form ;
  lemon:writtenRep     "associate"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_associate_with_3_present3rd a      lemon:Form ;
  lemon:writtenRep "associated"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_associate_with_3_past a lemon:Form ;
  lemon:writtenRep  "associated"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_associate_with_3_perfect a   lemon:Form ;
  lemon:writtenRep "associated"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:associate_with_3_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :associate_with_3_subj ;
  lexinfo:prepositionalAdjunct :associate_with_3_obj .

:associate_with_3_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :associate_with_3_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/movement> ;
  lemon:subjOfProp  :associate_with_3_subj ;
  lemon:objOfProp   :associate_with_3_obj ;
  lemon:condition   :associate_with_3_sense_1_condition .

:associate_with_3_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Artist> ;
  lemon:propertyRange  <http://www.w3.org/2002/07/owl#Thing> .


:associate_with_3_obj lemon:marker :form_associate_with_3_preposition .

## Prepositions ##
:form_associate_with_3_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "with"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

