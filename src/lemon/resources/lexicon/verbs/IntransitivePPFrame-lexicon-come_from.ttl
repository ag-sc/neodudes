@prefix :        <http://localhost:8000/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://localhost:8000/lemon.owl#> .

@base            <http://localhost:8000#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :come_from ;
  lemon:entry    :form_come_from_preposition .

:come_from a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_come_from ;
  lemon:otherForm      :form_come_from_present3rd ;
  lemon:otherForm      :form_come_from_past ;
  lemon:otherForm      :form_come_from_perfect ;
  lemon:sense          :come_from_sense_1 ;
  lemon:synBehavior    :come_from_frame .

:form_come_from a           lemon:Form ;
  lemon:writtenRep     "come"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_come_from_present3rd a      lemon:Form ;
  lemon:writtenRep "comes"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_come_from_past a lemon:Form ;
  lemon:writtenRep  "came"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_come_from_perfect a   lemon:Form ;
  lemon:writtenRep "come"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:come_from_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :come_from_subj ;
  lexinfo:prepositionalAdjunct :come_from_obj .

:come_from_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :come_from_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/nationality> ;
  lemon:subjOfProp  :come_from_subj ;
  lemon:objOfProp   :come_from_obj ;
  lemon:condition   :come_from_sense_1_condition .

:come_from_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .


:come_from_obj lemon:marker :form_come_from_preposition .

## Prepositions ##
:form_come_from_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "from"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

